{
  "name": "leetcode",
  "version": "1.0.1",
  "description": "My leetcode solutions.",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --write \"./**/*.{js,ts,yml}\" --ignore-path ./.prettierignore",
    "clean": "rimraf build",
    "ts-build": "npm run clean && tsc -p ./tsconfig.json",
    "test": "jest",
    "lint": "eslint --ext .js,.vue . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gengjiawen/leetcode.git"
  },
  "author": "gengjiawen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gengjiawen/leetcode/issues"
  },
  "homepage": "https://github.com/gengjiawen/leetcode#readme",
  "dependencies": {
    "axios": "0.19.0",
    "fs-extra": "7.0.1",
    "sequelize": "5.18.4",
    "sqlite3": "4.1.0",
    "to-markdown": "3.1.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)(spec|test).(ts|tsx|js)",
      "**/__tests__/*.(ts|tsx|js)",
      "**/test/*.(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.5",
    "@types/node": "13.1.0",
    "@types/jest": "*",
    "@types/rimraf": "2.0.2",
    "@types/sequelize": "^4.27.10",
    "@types/to-markdown": "3.0.0",
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "babel-jest": "24.9.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "jest": "24.9.0",
    "nodemon": "1.18.10",
    "prettier": "1.19.1",
    "rimraf": "2.6.3",
    "ts-jest": "24.1.0",
    "ts-node": "8.0.3",
    "typescript": "3.7.4"
  }
}
